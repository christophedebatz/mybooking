CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CONCERT(ID INTEGER NOT NULL PRIMARY KEY,DURATION INTEGER NOT NULL,TITLE VARCHAR(255),GROUP_ID INTEGER,BAND_ID INTEGER)
CREATE MEMORY TABLE CUSTOMER(ID INTEGER NOT NULL PRIMARY KEY,FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255),PASSWORD VARCHAR(255),USERNAME VARCHAR(255),EMAIL VARCHAR(70))
CREATE MEMORY TABLE CUSTOMERORDER(ID INTEGER NOT NULL PRIMARY KEY,DATEACHAT TIMESTAMP,STATUS INTEGER,CUSTOMER_ID INTEGER,CONSTRAINT FKAEF781F0A6F97AC6 FOREIGN KEY(CUSTOMER_ID) REFERENCES CUSTOMER(ID))
CREATE MEMORY TABLE PLACE(ID INTEGER NOT NULL PRIMARY KEY,PRICE FLOAT NOT NULL,TYPE VARCHAR(255),CUSTOMERORDER_ID INTEGER,REPRESENTATION_ID INTEGER,CONSTRAINT FK499E8E7B240554E FOREIGN KEY(CUSTOMERORDER_ID) REFERENCES CUSTOMERORDER(ID))
CREATE MEMORY TABLE REPRESENTATION(ID INTEGER NOT NULL PRIMARY KEY,DATE TIMESTAMP,CONCERT_ID INTEGER,SALLE_ID INTEGER,CONSTRAINT FK9C4724ED385E40AE FOREIGN KEY(CONCERT_ID) REFERENCES CONCERT(ID))
CREATE MEMORY TABLE SALLE(ID INTEGER NOT NULL PRIMARY KEY,CITY VARCHAR(255),COUNTRY VARCHAR(255),MAXCAPACITY INTEGER NOT NULL,NAME VARCHAR(255),ROAD VARCHAR(255),ZIPCODE INTEGER NOT NULL)
CREATE MEMORY TABLE GEN_ID(GEN_KEY VARCHAR(255),GEN_VALUE INTEGER)
CREATE MEMORY TABLE BAND(ID INTEGER NOT NULL PRIMARY KEY,DESCRIPTION VARCHAR(255),NAME VARCHAR(255),STYLE VARCHAR(255))
ALTER TABLE CONCERT ADD CONSTRAINT FK9BE81286182A0D66 FOREIGN KEY(BAND_ID) REFERENCES BAND(ID)
ALTER TABLE PLACE ADD CONSTRAINT FK499E8E731FC9766 FOREIGN KEY(REPRESENTATION_ID) REFERENCES REPRESENTATION(ID)
ALTER TABLE REPRESENTATION ADD CONSTRAINT FK9C4724EDD104C40E FOREIGN KEY(SALLE_ID) REFERENCES SALLE(ID)
